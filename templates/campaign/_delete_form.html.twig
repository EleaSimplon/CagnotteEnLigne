<form method="post" action="{{ path('campaign_delete', {'id': campaign.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ campaign.id) }}">
    <a class="btn btn-large waves-effect waves-light pink col s12">Delete</a>
</form>
