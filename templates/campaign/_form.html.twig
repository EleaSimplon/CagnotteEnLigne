{{ form_start(form) }}

{% if not inputValue %}
    {{ form_row(form.campaignName) }}
{% else %}
    {{ form_row(form.campaignName, {'attr' :  {'value' : inputValue } 
    }) }}
{% endif %}

    {{ form_row(form.campaignContent) }}
    {{ form_row(form.campaignGoal) }}
    {{ form_row(form.name) }}
    {{ form_row(form.email) }}
    
    <button class="btn">{{ button_label|default('Save') }}</button>
    
{{ form_end(form) }}